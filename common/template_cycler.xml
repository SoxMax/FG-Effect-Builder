<?xml version="1.0" encoding="iso-8859-1"?>

<root>
    <template name="button_stringcycler_defaultable">
        <button_stringcycler>
            <script file="common/scripts/button_stringcycler_defaultable.lua" />
        </button_stringcycler>
    </template>

    <template name="cycler_effect_editor">
        <button_stringcycler_defaultable>
            <anchored width="100" height="20">
                <top offset="2" />
                <left parent="leftanchor" anchor="right" relation="relative" offset="10" />
            </anchored>
            <sourceless />
        </button_stringcycler_defaultable>
    </template>

    <template name="cycler_effect_category">
        <cycler_effect_editor>
            <script file="common/scripts/cycler_effect_category.lua" />
        </cycler_effect_editor>
    </template>

    <template name="cycler_effect">
        <cycler_effect_editor>
            <script file="common/scripts/cycler_effect.lua" />
        </cycler_effect_editor>
    </template>

    <template name="stringcycler_effect">
        <cycler_effect_editor>
            <script file="common/scripts/valueholder_effect_editor.lua" />
        </cycler_effect_editor>
    </template>

    <template name="cycler_effect_subwindow">
        <stringcycler_effect name="effect">
            <script file="common/scripts/cycler_effect_subwindow.lua" />
        </stringcycler_effect>
    </template>

    <template name="cycler_range">
        <stringcycler_effect name="effect_range">
            <parameters>
                <defaultlabelres mergerule="replace">allrange</defaultlabelres>
                <labelsres>melee|ranged</labelsres>
                <values>melee|ranged</values>
            </parameters>
        </stringcycler_effect>
    </template>
</root>
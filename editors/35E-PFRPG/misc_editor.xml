<?xml version="1.0" encoding="iso-8859-1"?>

<root>
    <windowclass name="misc_editor">
        <script file="editors/35E-PFRPG/scripts/misc_editor.lua" />
        <sheetdata>
            <anchor_column name="columnanchor" />

            <effects_stringcycler name="misc">
                <anchored>
                    <top parent="columnanchor" anchor="bottom" relation="relative" offset="7" />
                    <left offset="5" />
                    <right offset="-230" />
                </anchored>
                <tooltip textres="effect_label_misc_type_tooltip" />
                <parameters>
                    <defaultlabelres mergerule="replace">ac</defaultlabelres>
                    <defaultvalue>AC</defaultvalue>
                    <labelsres>atk|dmg|heal|init_ae</labelsres>
                    <values>ATK|DMG|HEAL|INIT</values>
                </parameters>
            </effects_stringcycler>

            <effect_number name="misc_modifier">
                <anchored to="misc" position="righthigh" offset="10,0" width="20" height="20"/>
                <tooltip textres="effect_label_misc_value_tooltip" />
            </effect_number>

            <combobox_bonus_types name="misc_bonus_type">
                <anchored to="misc_modifier" position="righthigh" offset="10,0" width="100" height="20"/>
            </combobox_bonus_types>
        </sheetdata>
    </windowclass>
</root>
